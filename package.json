{
  "name": "rotorua-dark",
  "displayName": "Rotorua Dark",
  "description": "The best vs code theme",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently 'tsc -w --preserveWatchOutput' 'nodemon ./dist/genTheme.js'",
    "vscode:prepublish": "tsc && node ./dist/genTheme.js"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Rotorua",
        "uiTheme": "vs-dark",
        "path": "./themes/Rotorua-color-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martinmunillas/rotorua-vscode-theme.git"
  },
  "publisher": "Rotorua",
  "keywords": [
    "theme",
    "dark theme",
    "martinmunilla",
    "rotorua"
  ],
  "devDependencies": {
    "@types/node": "^14.14.37",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.3"
  }
}
